import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner io=new Scanner(System.in);
        int n=io.nextInt();
        int a=0;
        int b=1;
        int c=a+b;
        for(int i=0;i<n-1;i++){
            
            a=b;
            b=c;
            c=a+b;
        }
        System.out.println(c);
        
    }
}
